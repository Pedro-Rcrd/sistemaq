        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CodigoCarreraNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CodigoCarreraNavigation.CodigoCarrera)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CodigoEstablecimientoNavigation)
