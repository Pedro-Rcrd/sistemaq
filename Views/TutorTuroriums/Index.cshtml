@model IEnumerable<systemquchooch.Models.TutorTurorium>

@{
    ViewData["Title"] = "Index";
}

<h1 class="text-center">Tutor-Tutoría</h1>

<p class="text-end">
    <a class="btn btn-outline-success" asp-controller="TutorTuroriums" asp-action="Create">Nuevo Tutor-Tutoría</a>
    <a class="btn btn-success" asp-controller="TutorTuroriums" asp-action="Index">Lista de Tutor-Tutoría</a>

</p>
<hr class="LineaRecta" />

<p>
    <a asp-controller="TutorTuroriums" asp-action="ImprimirTutor_tutoria" target="_blank" class="btn btn-success">Generar PDF</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CodigoTutorNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CodigoTutoriaNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CodigoTutorNavigation.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CodigoTutoriaNavigation.CodigoTutoria)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CodigoTutorTutoria">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CodigoTutorTutoria">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CodigoTutorTutoria">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
