@model IEnumerable<systemquchooch.Models.EstudianteTutorium>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>

<p>
    <a asp-controller="EstudianteTutoriums" asp-action="ImprimirEstudiante_tutoria" target="_blank" class="btn btn-info text-white">Generar PDF</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CodigoEstudianteNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CodigoTutoriaNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CodigoEstudianteNavigation.CodigoEstudiante)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CodigoTutoriaNavigation.CodigoTutoria)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CodigoEstudianteTutoria">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CodigoEstudianteTutoria">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CodigoEstudianteTutoria">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
